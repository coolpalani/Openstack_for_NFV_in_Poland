/* The functions below expand and collapse comments  */

function toggle_comment_display(link, comment_id) {
    var comment =3D document.getElementById('comment_text_' + comment_id);
    var re =3D new RegExp(/\bcollapsed\b/);
    if (comment.className.match(re))
        expand_comment(link, comment);
    else
        collapse_comment(link, comment);
}

function toggle_all_comments(action) {
    // If for some given ID the comment doesn't exist, this doesn't mean
    // there are no more comments, but that the comment is private and
    // the user is not allowed to view it.

    var comments =3D YAHOO.util.Dom.getElementsByClassName('bz_comment_text=
');
    for (var i =3D 0; i < comments.length; i++) {
        var comment =3D comments[i];
        if (!comment)
            continue;

        var id =3D comments[i].id.match(/\d*$/);
        var link =3D document.getElementById('comment_link_' + id);
        if (action =3D=3D 'collapse')
            collapse_comment(link, comment);
        else
            expand_comment(link, comment);
    }
}

function collapse_comment(link, comment) {
    link.innerHTML =3D "[+]";
    YAHOO.util.Dom.addClass(comment, 'collapsed');
}

function expand_comment(link, comment) {
    link.innerHTML =3D "[&minus;]";
    YAHOO.util.Dom.removeClass(comment, 'collapsed');
}

function wrapReplyText(text) {
    // This is -3 to account for "\n> "
    var maxCol =3D BUGZILLA.constant.COMMENT_COLS - 3;
    var text_lines =3D text.replace(/[\s\n]+$/, '').split("\n");
    var wrapped_lines =3D new Array();

    for (var i =3D 0; i < text_lines.length; i++) {
        var paragraph =3D text_lines[i];
        // Don't wrap already-quoted text.
        if (paragraph.indexOf('>') =3D=3D 0) {
            wrapped_lines.push('> ' + paragraph);
            continue;
        }

        var replace_lines =3D new Array();
        while (paragraph.length > maxCol) {
            var testLine =3D paragraph.substring(0, maxCol);
            var pos =3D testLine.search(/\s\S*$/);

            if (pos < 1) {
                // Try to find some ASCII punctuation that's reasonable
                // to break on.
                var punct =3D '\\-\\./,!;:';
                var punctRe =3D new RegExp('[' + punct + '][^' + punct + ']=
+$');
                pos =3D testLine.search(punctRe) + 1;
                // Try to find some CJK Punctuation that's reasonable
                // to break on.
                if (pos =3D=3D 0)
                    pos =3D testLine.search(/[\u3000\u3001\u3002\u303E\u303=
F]/) + 1;
                // If we can't find any break point, we simply break long
                // words. This makes long, punctuation-less CJK text wrap,
                // even if it wraps incorrectly.
                if (pos =3D=3D 0) pos =3D maxCol;
            }

            var wrapped_line =3D paragraph.substring(0, pos);
            replace_lines.push(wrapped_line);
            paragraph =3D paragraph.substring(pos);
            // Strip whitespace from the start of the line
            paragraph =3D paragraph.replace(/^\s+/, '');
        }
        replace_lines.push(paragraph);
        wrapped_lines.push("> " + replace_lines.join("\n> "));
    }
    return wrapped_lines.join("\n") + "\n\n";
}

