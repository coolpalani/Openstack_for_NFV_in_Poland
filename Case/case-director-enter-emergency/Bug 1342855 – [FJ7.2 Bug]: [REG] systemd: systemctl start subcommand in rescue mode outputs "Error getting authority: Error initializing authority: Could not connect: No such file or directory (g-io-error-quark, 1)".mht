/* Concatenates all text from element's childNodes. This is used
 * instead of innerHTML because we want the actual text (and
 * innerText is non-standard).
 */
function getText(element) {
    var child, text =3D "";
    for (var i=3D0; i < element.childNodes.length; i++) {
        child =3D element.childNodes[i];
        var type =3D child.nodeType;
        if (type =3D=3D Node.TEXT_NODE || type =3D=3D Node.ENTITY_REFERENCE=
_NODE) {
            text +=3D child.nodeValue;
        } else {
            /* recurse into nodes of other types */
            text +=3D getText(child);
        }
    }
    return text;
}

